{
	"builds": [
		{
			"src": "chatbot/chatbot/wsgi.py",
			"use": "@vercel/python",
			"config": { "maxLambdaSize": "15mb", "runtime": "python3.9" },
			"buildCommand": "bash setup.sh"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "chatbot/chatbot/wsgi.py"
		},
		{
			"src": "/static/(.*)",
			"dest": "chatbot/static/$1"
		}
	]
}